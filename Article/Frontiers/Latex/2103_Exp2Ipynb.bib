issn = {01677799},
journal = {Trends Biotechnol.},
keywords = {Comparative Study,Gene Expression / genetics*,Gene Library*,Genetic / genetics*,Genetic Engineering / methods,Genetic Engineering / trends,Ivan Mijakovic,Karin Hammer,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,Peter Ruhdal Jensen,Promoter Regions,PubMed Abstract,Research Support,doi:10.1016/j.tibtech.2005.12.003,pmid:16406119},
number = {2},
pages = {53--55},
pmid = {16406119},
publisher = {Elsevier Ltd},
title = {{Synthetic promoter libraries - Tuning of gene expression}},
url = {https://pubmed.ncbi.nlm.nih.gov/16406119/},
volume = {24},
year = {2006}
}
@article{Fortin2012,
abstract = {DEAP is a novel evolutionary computation framework for rapid prototyping and testing of ideas. Its design departs from most other existing frameworks in that it seeks to make algorithms explicit and data structures transparent, as opposed to the more common black-box frameworks. Freely available with extensive documentation at http://deap.gel.ulaval.ca, DEAP is an open source project under an LGPL license.},
