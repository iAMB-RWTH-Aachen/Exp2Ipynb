doi = {10.1109/ICTAI.2019.00209},
eprint = {1908.05557},
isbn = {978-1-7281-3798-8},
issn = {10823409},
keywords = {AutoML,Automated machine learning,Driverless AI,Hyperparameter optimization,Model selection},
pages = {1471--1479},
